    "del": "^2.2.1",
    "gulp-clean-css": "^2.0.10",
    "gulp-concat-css": "^2.3.0",
    "gulp-concat-util": "^0.5.5",
    "gulp-gh-pages": "^0.5.4",
    "gulp-insert": "^0.5.0",
