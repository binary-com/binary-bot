    "gulp-rename": "^1.2.2",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "^5.0.1",
    "husky": "^2.3.0",
    "i18next-scanner": "^1.9.4",
