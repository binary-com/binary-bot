    "bootstrap": "^3.3.7",
    "filesaverjs": "^0.2.2",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-open": "^2.0.0",
    "jquery": "~2.2.4",
    "notifyjs-browser": "^0.4.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "sha1": "^1.1.1",
    "tourist": "easelinc/tourist.git#a31cf497cc7af8173513ea3f57f760894d5262cb",
    "trackjs": "^2.4.0",
    "underscore": "^1.8.3"
  },
