    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
