    "gulp-watch": "^4.3.8",
    "gulp-webpack": "^1.5.0",
    "i18next-scanner": "^1.4.1",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "jshint": "^2.9.2",
